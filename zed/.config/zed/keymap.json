[
  {
    "context": "Editor",
    "bindings": {
      "ctrl-y": "editor::ConfirmCompletion",
      "ctrl-j": ["workspace::SendKeystrokes", "down"],
      "ctrl-k": ["workspace::SendKeystrokes", "up"],
      "ctrl-a": "editor::ShowCompletions"
    }
  },
  {
    "context": "Editor && vim_mode == normal",
    "bindings": {
      "space r": "workspace::NewSearch",
      "space k": "editor::ToggleCodeActions",
      "space h": "workspace::ToggleLeftDock",
      "; a": "workspace::ToggleRightDock",
      "ctrl-d": ["workspace::SendKeystrokes", "ctrl-d z z"],
      "ctrl-u": ["workspace::SendKeystrokes", "ctrl-u z z"],
      "space j": "tab_switcher::Toggle"
    }
  },
  {
    "context": "Editor && VimControl && !VimWaiting && !menu",
    "bindings": {
      "; j": "terminal_panel::ToggleFocus"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "; j": "terminal_panel::Toggle"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "; f": "file_finder::Toggle"
    }
  },
  {
    "bindings": {
      "; o": "project_symbols::Toggle"
    }
  },
  {
    "bindings": {
      "; a": "agent::ToggleFocus"
    }
  },
  {
    "context": "Workspace",
    "bindings": {
      "; p": "command_palette::Toggle"
    }
  },
  {
    "bindings": {
      "; space": "command_palette::Toggle"
    }
  },
  {
    "bindings": {
      "; i": "outline::Toggle"
    }
  },
  {
    "bindings": {
      "; p": "project_panel::ToggleFocus"
    }
  },
  {
    "context": "((vim_mode == helix_normal || vim_mode == helix_select) && !menu)",
    "bindings": {
      "space f": null
    }
  }
]
