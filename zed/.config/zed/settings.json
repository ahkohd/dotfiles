{
  "ui_font_weight": 450.0,
  "disable_ai": true,
  "preview_tabs": {
    "enabled": false
  },
  "minimap": {
    "max_width_columns": 65,
    "thumb": "hover",
    "show": "never"
  },
  "gutter": {
    "line_numbers": false
  },
  "show_signature_help_after_edits": true,
  "auto_signature_help": true,
  "scroll_sensitivity": 1.0,
  "sticky_scroll": {
    "enabled": true
  },
  "base_keymap": "VSCode",
  "buffer_font_weight": 400.0,
  "icon_theme": {
    "mode": "system",
    "light": "Zed (Default)",
    "dark": "Zed (Default)"
  },
  "collaboration_panel": {
    "button": false
  },
  "notification_panel": {
    "button": false
  },
  "git_panel": {
    "button": false
  },
  "outline_panel": {
    "button": false,
    "git_status": false,
    "folder_icons": false,
    "file_icons": false
  },
  "use_system_window_tabs": true,
  "bottom_dock_layout": "contained",
  "title_bar": {
    "show_branch_icon": false,
    "show_sign_in": false,
    "show_user_picture": false,
    "show_onboarding_banner": false,
    "show_project_items": false,
    "show_branch_name": false
  },
  "diagnostics": {
    "button": false
  },
  "debugger": {
    "button": false
  },
  "search": {
    "button": false
  },
  "project_panel": {
    "indent_size": 11.0,
    "git_status": false,
    "folder_icons": false,
    "file_icons": false,
    "button": false,
    "indent_guides": { "show": "never" }
  },
  "show_wrap_guides": true,
  "soft_wrap": "editor_width",
  "ui_font_family": "TX-02-AREMU",
  "agent": {
    "dock": "left",
    "button": false,
    "play_sound_when_agent_done": true,
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    }
  },
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "vim_mode": true,
  "relative_line_numbers": "enabled",
  "cursor_blink": false,
  "ui_font_size": 14.0,
  "buffer_font_size": 13.0,
  "buffer_font_family": "TX-02-AREMU",
  "buffer_line_height": "standard",
  "terminal": {
    "dock": "bottom",
    "button": false,
    "font_family": "TX-02-AREMU",
    "font_size": 13.0
  },
  "theme": "Everforest Dark Hard (blur)",
  "scrollbar": {
    "show": "never"
  },
  "vertical_scroll_margin": 0,
  "inlay_hints": {
    "enabled": false
  },
  "indent_guides": {
    "enabled": false
  },
  "toolbar": {
    "agent_review": false,
    "code_actions": false,
    "selections_menu": false,
    "breadcrumbs": false,
    "quick_actions": false
  },
  "tab_bar": {
    "show_nav_history_buttons": false,
    "show": false
  },
  "status_bar": {
    "experimental.show": false,
    "cursor_position_button": false,
    "active_language_button": false
  },
  "current_line_highlight": "none"
}
