{
  "agent": {
    "play_sound_when_agent_done": true,
    "default_model": {
      "provider": "zed.dev",
      "model": "claude-3-5-sonnet-latest"
    }
  },
  "telemetry": {
    "metrics": false,
    "diagnostics": false
  },
  "vim_mode": true,
  "relative_line_numbers": true,
  "cursor_blink": false,
  "ui_font_size": 14,
  "buffer_font_size": 14,
  "buffer_font_family": "TX-02-AREMU",
  "buffer_line_height": {
    "custom": 1.8
  },
  "terminal": {
    "font_family": "TX-02-AREMU",
    "font_size": 14,
    "line_height": {
      "custom": 1.7
    }
  },
  "theme": {
    "mode": "system",
    "light": "One Light",
    "dark": "Tokyo Night"
  },
  "scrollbar": {
    "show": "never"
  },
  "vertical_scroll_margin": 0,
  "inlay_hints": {
    "enabled": false
  },
  "indent_guides": {
    "enabled": false
  },
  "toolbar": {
    "breadcrumbs": true,
    "quick_actions": false
  },
  "tab_bar": {
    "show": false
  },
  "status_bar": {
    "cursor_position_button": false,
    "active_language_button": false
  },
  "current_line_highlight": "none"
}
